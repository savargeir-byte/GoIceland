# GO ICELAND - Environment Configuration
# Copy this file to .env and fill in your values

# Firebase Configuration
FIRESTORE_PROJECT_ID=go-iceland
FIREBASE_BUCKET=go-iceland.appspot.com
FIREBASE_SERVICE_ACCOUNT=./scripts/serviceAccountKey.json

# API Keys
MAPBOX_TOKEN=pk.YOUR_MAPBOX_TOKEN_HERE

# Overpass API (OpenStreetMap data source)
OVERPASS_API=https://overpass-api.de/api/interpreter

# Data Processing
BATCH_SIZE=500
MAX_RETRIES=3
RATE_LIMIT_DELAY=2

# Map Preview Settings
MAP_PREVIEW_WIDTH=400
MAP_PREVIEW_HEIGHT=300
MAP_PREVIEW_ZOOM=13

# Geohash Settings
GEOHASH_PRECISION=9
